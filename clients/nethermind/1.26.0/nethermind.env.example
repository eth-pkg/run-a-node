# The max number of cached hashes of already known transactions. Set automatically by the memory hint. (DEFAULT: 524288)
NETHERMIND_TxPool_HashCacheSize=

# The max number of full blob transactions stored in memory. Used only if persistent storage is disabled. (DEFAULT: 512)
NETHERMIND_TxPool_InMemoryBlobPoolSize=

# The max number of pending blob transactions per single sender. `0` to lift the limit. (DEFAULT: 16)
NETHERMIND_TxPool_MaxPendingBlobTxsPerSender=

# The max number of pending transactions per single sender. `0` to lift the limit. (DEFAULT: 0)
NETHERMIND_TxPool_MaxPendingTxsPerSender=

# The minimal percentage of the current base fee that must be surpassed by the max fee (`max_fee_per_gas`) for the transaction to be broadcasted. (DEFAULT: 70)
NETHERMIND_TxPool_MinBaseFeeThreshold=

# The average percentage of transaction hashes from persistent broadcast sent to a peer together with hashes of the last added transactions. (DEFAULT: 5)
NETHERMIND_TxPool_PeerNotificationThreshold=

# The max number of full blob transactions stored in the database (increasing the number of transactions in the blob pool also results in higher memory usage). The default value uses max 13GB for 6 blobs where one blob is 2GB (16386 * 128KB). (DEFAULT: 16384)
NETHERMIND_TxPool_PersistentBlobStorageSize=

# The current transaction pool state reporting interval, in minutes. (DEFAULT: null)
NETHERMIND_TxPool_ReportMinutes=

# The max number of transactions held in the mempool (the more transactions in the mempool, the more memory used). (DEFAULT: 2048)
NETHERMIND_TxPool_Size=

# The number of autogenerated developer accounts to work with. Developer accounts have private keys from `00...01` to `00...n`. (DEFAULT: 10)
NETHERMIND_Wallet_DevAccounts=
